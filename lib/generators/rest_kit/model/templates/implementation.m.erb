#import "<%= filename %>.h"

@implementation <%= filename %>

@dynamic <%= columns.map{|c| ios_attr_name(c.name) }.join(", ") %>;
<% if associations.length > 0 %>
@dynamic <%= associations.map{|a| ios_attr_name(a.name) }.join(", ") %>;
<% end %>
<% if has_many_associations.length > 0 -%>
@dynamic <%= has_many_associations.map{|a| ios_attr_name(a.name.to_s.singularize + "_ids") }.join(", ") %>;
<% end -%>

@end