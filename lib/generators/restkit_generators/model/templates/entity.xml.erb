<entity name="<%= model_name %>" representedClassName="<%= model_name %>" syncable="YES">
<% columns.each do |column| -%>
  <attribute name="<%= ios_attr_name(column.name) %>" optional="YES" attributeType="<%= core_data_type(column.type) %>" syncable="YES"/>
<% end -%>
<% associations.each do |ass| -%>
  <relationship name="<%= ios_attr_name(ass.name) %>" optional="YES" <% if macro_to_many?(ass.macro) %>toMany="YES"<% end %> deletionRule="Nullify" destinationEntity="<%= ios_class_name(ass.name) %>" syncable="YES"/>  
<% end -%>
</entity>
